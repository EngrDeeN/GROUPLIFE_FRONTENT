
<form method="post" action="/Policy_Claims/SaveUpdateAgent" class="form-horizontal">
    <div class="card">
        <div class="card-body">
            <h1 class="card-title mt-2" style="font-weight:600; font-size:1.4rem">
                CLAIM
            </h1>
        </div>
        <div class="card-body">
            <div class="row ">
                <div class="mb-2 col-md-3">
                    <label class="text-success" for=" " style="font-weight:bold">Membership No:</label>
                    <input type="number" class="form-control" name="" id="" value=" ">
                </div>
                <div class="mb-3 col-md-3">
                    <label class="text-success" for="" style="font-weight:bold">CNIC NO:</label>
                    <input type="number" class="form-control" name="" id="" value=" ">
                </div>
                <div class="mb-2 col-md-3">
                    <div> <label class="text-success" for=" " style="font-weight:bold"></label></div>
                    <button class="btn btn-primary" type="button" id="" name="" onclick="" value="Search">Search!</button>
                </div>
            </div>
        </div>
        <div class="card-body bg-primary">
            <div class="row">
                <h1 class="tab-title text-white" style="font-weight:bold"><strong><u>CLAIM INTIMATION:</u></strong></h1>
            </div>
            <div class="row mt-1">
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Claim No:</label>
                    <input id="" name="" placeholder="Claim No" class="form-control" readonly value="@TempData[""]" />
                </div>
            </div>
            <div class="row mt-1">
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Date of Event:</label>
                    <input type="date" id="" name="" class="datefield form-control" />
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Cause of Event:</label>
                    <input id="" name="" class="form-control" placeholder="Cause of Event" type="text">
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Place of Event:</label>
                    <input id="" name="" class="form-control" placeholder="Place of Event" type="text">
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Date of Intimation:</label>
                    <input type="date" id="" name="" class="datefield form-control" />
                </div>
            </div>
            <div class="row mt-1">
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Claim Type:</label>
                    <select id="" name="" class="form-select">
                        <option value="">--Select--</option>
                        <option value="1">Basic Death</option>
                        <option value="2">Accidential Death</option>
                        <option value="3">Accidential Disability</option>
                    </select>
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Remarks:</label>
                    <input id="" name="" class="form-control" placeholder="Remarks" type="text">
                </div>
            </div>
            <div class="row mt-3">
                <h1 class="tab-title text-white" style="font-weight:bold"><strong><u>MEMBERSHIP INFO:</u></strong></h1>
            </div>
            <div class="row mt-1">
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Coverage Period From:</label>
                    <input type="date" id="" name="" class="datefield form-control" />
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Coverage Period To:</label>
                    <input type="date" id="" name="" class="datefield form-control" />
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Membership Status:</label>
                    <input id="" name="" class="form-control" placeholder="Membership Status" type="text">
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mt-1" style="font-weight:bold">Free Cover Limit:</label>
                    <input id="" name="" class="form-control" placeholder="Free Cover Limit" type="number">
                </div>
            </div>
            <div class="row mt-2">
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Covered Benefits:</label>
                    <input id="" name="" class="form-control" placeholder="Covered Benefits" type="text">
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">No. of Enployee:</label>
                    <input id="" name="" class="form-control" placeholder="No. of Employee" type="number">
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Contribution:</label>
                    <input id="" name="" class="form-control" placeholder="No. of Employee" type="number">
                </div>
            </div>
            <div class="row mt-3 ">
                <h1 class="tab-title text-white" style="font-weight:bold"><strong><u>COVERED MEMBER'S INFO:</u></strong></h1>
            </div>
            <div class="row mt-1">
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Name:</label>
                    <input id="" name="" class="form-control" placeholder="Name" type="text">
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Employee ID:</label>
                    <input id="" name="" class="form-control" placeholder="Employee ID" type="number">
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Date of Birth:</label>
                    <input type="date" id="" name="" class="datefield form-control" />
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Status:</label>
                    <input id="" name="" class="form-control" placeholder="Status" type="text">
                </div>

            </div>
            <div class="row mt-1">
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Enrollment Date:</label>
                    <input type="date" id="" name="" class="datefield form-control" />
                </div>
                <div class="col-md-3">
                    <label for="" class="text-white mb-1" style="font-weight:bold">Category:</label>
                    <input id="" name="" class="form-control" placeholder="Category" type="text">
                </div>
            </div>
            <div class="row mt-3">
                <div class="mb-2 col-md-5">
                    <button class="btn btn-success" style="margin-left:01%; width:20%; text-align: left;" type="submit"
                            id="" name="">
                        Process
                    </button>
                </div>
            </div>
        </div>
    </div>
</form>