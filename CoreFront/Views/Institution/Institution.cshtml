<script type="text/javascript">

    $(document).ready(function () {
        var next = 0;

        $("#btnDirInfo").click(function (e) {
            e.preventDefault();
            if ($("#TableDirInfo tbody").length == 0) {
                $("#TableDirInfo").append("<tbody></tbody>");
            }
            next += 1;
            var totalRowCount = 0;
            var rowCount = 0;
            var table = document.getElementById("TableDirInfo");
            var rows = table.getElementsByTagName("tr")
            for (var i = 0; i < rows.length; i++) {
                totalRowCount++;
                if (rows[i].getElementsByTagName("td").length > 0) {
                    rowCount++;
                }
            }
            if (totalRowCount > 0) {
                next = totalRowCount + 1;
            }
            if (i == 0) {
                $("#TableDirInfo tbody").append("<tr><td class='text-white' style='font-weight:bold'>Full Name:</td>" +
                    "<td class='text-white' style='font-weight:bold'>Designation:</td>" +
                    "<td class='text-white' style='font-weight:bold'>Email:</td > " +
                    "<td class='text-white' style='font-weight:bold'>CNIC #:</td > " +
                    "<td class='text-white' style='font-weight:bold'>CNIC Issuance:</td > " +
                    "<td class='text-white' style='font-weight:bold'>CNIC Expiry:</td>" +
                    "<td class='text-white' style='font-weight:bold'>Delete Info:</td>" +
                    "</tr>");

            }
            // Append product to the table
            $("#TableDirInfo tbody").append("<tr>" +
                "<td><input class='form-control' style='width:200px;' placeholder='Full Name'    maxlength='25' name='FSIO_OWNER_NAME' id='FSIO_OWNER_NAME" + next + "'></td>" +
                "<td><input class='form-control' style='width:200px;' placeholder='Designation'  maxlength='25' name='FSIO_OWNER_DESIGN' id='FSIO_OWNER_DESIGN" + next + "'></td>" +
